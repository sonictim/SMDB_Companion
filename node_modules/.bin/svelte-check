#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tfarrell/Documents/CODE/SMDB_Companion/node_modules/.pnpm/svelte-check@4.1.7_picomatch@4.0.2_svelte@3.59.2_typescript@5.6.3/node_modules/svelte-check/bin/node_modules:/Users/tfarrell/Documents/CODE/SMDB_Companion/node_modules/.pnpm/svelte-check@4.1.7_picomatch@4.0.2_svelte@3.59.2_typescript@5.6.3/node_modules/svelte-check/node_modules:/Users/tfarrell/Documents/CODE/SMDB_Companion/node_modules/.pnpm/svelte-check@4.1.7_picomatch@4.0.2_svelte@3.59.2_typescript@5.6.3/node_modules:/Users/tfarrell/Documents/CODE/SMDB_Companion/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tfarrell/Documents/CODE/SMDB_Companion/node_modules/.pnpm/svelte-check@4.1.7_picomatch@4.0.2_svelte@3.59.2_typescript@5.6.3/node_modules/svelte-check/bin/node_modules:/Users/tfarrell/Documents/CODE/SMDB_Companion/node_modules/.pnpm/svelte-check@4.1.7_picomatch@4.0.2_svelte@3.59.2_typescript@5.6.3/node_modules/svelte-check/node_modules:/Users/tfarrell/Documents/CODE/SMDB_Companion/node_modules/.pnpm/svelte-check@4.1.7_picomatch@4.0.2_svelte@3.59.2_typescript@5.6.3/node_modules:/Users/tfarrell/Documents/CODE/SMDB_Companion/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/svelte-check@4.1.7_picomatch@4.0.2_svelte@3.59.2_typescript@5.6.3/node_modules/svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../.pnpm/svelte-check@4.1.7_picomatch@4.0.2_svelte@3.59.2_typescript@5.6.3/node_modules/svelte-check/bin/svelte-check" "$@"
fi
